#include<stdio.h>
#include<math.h>
int main()
{
    long int n,sum=0,remainder,x;
    scanf("%ld",&n);
    x=floor(log10(n)+1);
    if(x>5)
    {
    }
    else
    {
        while(n!=0)
        {
        remainder=n%10;
        sum=sum+remainder;
        n=n/10;
            
        }
    }
    printf("%d",sum);
    return 0;
    
 }
