#include<stdio.h>
#include<math.h>
void main()
{
    int n,sum=0,remainder,x;
    printf("enter the value of n");
    scanf("%d",&n);
    x=floor(log10(n)+1);
    if(x>5)
    {
      printf("input exceeds 5 digits");
    }
    else
    {
        while(n!=0)
        {
        remainder=n%10;
        sum=sum+remainder;
        n=n/10;
            
        }
    printf("the sum is %d",sum);
        
    }
}
        
    
