#include<stdio.h>
void main()
{
    int a[10],n,i;
    printf("enter the size of an array");
    scanf("%d",&n);
    if(n>10)
    {
        printf("array size is exceed\n");
    }
    printf("enter the array elements\n");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("the elements are \n");
    for(i=0;i<n;i++)
    {
        printf("%d\t",a[i]);
    }
    
}
